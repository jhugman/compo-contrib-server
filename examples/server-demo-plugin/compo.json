{
  "manifest_version": "1.0",
  "extensions": [
    {
      "epID": "http.endpoint",
      "path": "/u/:user",
      "method": "get",
      "function": "./lib/endpoints!getUser",
      "description": "Fake user profile page"
    },
    {
      "epID": "http.endpoint",
      "path": "/r",
      "directory": "__dirname/resources",
      "description": "Plugin resource directory"
    }
  ]
}